class Main {
  public static void main(String[] args) {

    System.out.println(fizzBuzz(9));
    System.out.println(fizzBuzz(1));
    System.out.println(fizzBuzz(15));
  }
    
    public static String fizzBuzz(int num) {
      if (num % 3 == 0) {
        return "fizz";
      }
      else if (num % 5 == 0){
        return "Buzz";
      }
      else{
        return "No fizz or Buzz";
      }
}
}
